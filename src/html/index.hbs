<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>
	{{> base/analytics }}

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}
</head>

{{> base/header 'sticky'}}

<body>

	<main id='content'>
		<section id='main'>
			{{> story/main}}
		</section>
		<section id='bubblechart'>
			{{> story/bubblechart}}
		</section>
		<!-- Share buttons code -->
        <div id="social__buttons">
            <a class="twitter-share-button"
                data-size="small"
                data-url='{{meta.site}}'
                data-hashtags="dataviz"
                >Tweet
            </a>
            <div class="fb-share-button" 
                data-href='{{meta.site}}'
                data-layout="button" 
                data-size="small">
                <a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Fwww.trendspotting.site%2F2020%2FMarch%2Femissions-mini%2Findex.html&amp;src=sdkpreparse" class="fb-xfbml-parse-ignore">Share</a>
            </div>
        </div>
		</br>
		</br>
		{{!-- <section id='epilogue'>	
			{{> story/epilogue}}
		</section> --}}

	</main>
	{{!-- {{> base/signature }} --}}
	{{!-- {{> base/footer }} --}}

	<script src='{{basepath}}assets/scripts/d3.v5.11.0.min.js'></script>
	<script src='{{basepath}}assets/scripts/d3.v4.12.0+jetpack.min.js'></script>
	<script src='{{basepath}}assets/scripts/canvas-toBlob.js'></script>
	<script src='{{basepath}}assets/scripts/FileSaver.min.js'></script>
	<script src='{{basepath}}assets/scripts/d3plus-text.v0.9.full.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>